<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/home"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/blue"
        tools:context=".ui.homeAuxliries.HomeFragment">
    <TextView
            android:layout_width="59dp"
            android:layout_height="0dp"
            android:id="@+id/textView"
            android:textColor="@color/white"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:text="Forms" android:layout_marginTop="112dp"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/white"
            android:id="@+id/textView2" app:layout_constraintEnd_toStartOf="@+id/divider2"
            android:layout_marginEnd="91dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintBottom_toTopOf="@+id/divider" android:layout_marginBottom="44dp" android:text="FLHA"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/white"
            android:id="@+id/textView3" app:layout_constraintStart_toEndOf="@+id/divider2"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintBottom_toTopOf="@+id/divider" android:layout_marginBottom="44dp"
            app:layout_constraintHorizontal_bias="0.503" android:text="SOS"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/white"
            android:id="@+id/textView4" app:layout_constraintEnd_toStartOf="@+id/divider2"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintTop_toBottomOf="@+id/divider"
            android:layout_marginTop="32dp" app:layout_constraintHorizontal_bias="0.507" android:text="PRODUCTIVITY"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/white"
            android:id="@+id/textView5" app:layout_constraintStart_toEndOf="@+id/divider2"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginTop="32dp" app:layout_constraintTop_toBottomOf="@+id/divider"
            app:layout_constraintHorizontal_bias="0.503" android:text="INSTRUCTION"/>
    <View
            android:id="@+id/divider"
            android:layout_width="264dp"
            android:layout_height="1dp"
            android:background="@color/white"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:layout_marginTop="248dp"
            app:layout_constraintTop_toTopOf="@+id/textView" app:layout_constraintHorizontal_bias="0.496"/>
    <View
            android:id="@+id/divider2"
            android:layout_width="1dp"
            android:layout_height="201dp"
            android:background="@color/white"
            android:visibility="visible" android:layout_marginTop="132dp"
            app:layout_constraintTop_toBottomOf="@+id/textView"
            android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>
